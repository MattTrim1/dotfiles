############
###  ls  ###
############
alias la="ls -halt"
alias l="la"
alias ll="la"

###########
### git ###
###########
alias gst="git status"
alias gcm="git commit -m"
alias gp="git push -u origin HEAD"
alias gl="git pull"
alias gf="git fetch"
alias glog="git log --all --decorate --oneline --graph"
alias ga="git add"
alias gaa="git add ."
